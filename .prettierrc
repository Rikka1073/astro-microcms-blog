{
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "printWidth": 200,
        "bracketSameLine": false
      }
    },
    {
      "files": ["*.jsx", "*.tsx"],
      "options": {
        "printWidth": 200,
        "bracketSameLine": false
      }
    }
  ]
}
